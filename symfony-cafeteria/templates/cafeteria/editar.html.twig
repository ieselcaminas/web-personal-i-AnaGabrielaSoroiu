{% extends 'base.html.twig' %}
{% block title %}Editar cafeteria{% endblock %}
{% block body %}
    <h1>Editar Cafeteria</h1>
    {{ form_start(formulario, {'attr': {'novalidate': 'novalidate'}}) }}
        {{ form_row(formulario.nombre) }}
        {{ form_row(formulario.calle) }}
        {{ form_row(formulario.telefono) }}
        {{ form_row(formulario.email) }}

        <button> Guardar cambios</button>
        <a href="{{ path('inicio') }}"> Cancelar</a>
    {{ form_end(formulario) }}    
{% endblock %}